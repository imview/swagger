<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gene.dao.mapper.UserMapper">

    <select id="getUserByRawHeadImgUrl" resultType="com.gene.dao.entity.User" parameterType="String">
        select * from user
        where raw_head_img_url = #{rawHeadImgUrl}
    </select>

</mapper>